  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/panter/catladder.git"
  },
  "bugs": "https://github.com/panter/catladder/issues",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-trailing-function-commas": "^6.20.0",
    "eslint": "^4.3.0",
